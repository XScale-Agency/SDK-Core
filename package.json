{
  "name": "@xscale/package-kit",
  "description": "",
  "version": "0.0.0",
  "private": true,
  "engines": {
    "node": ">=20.6.0"
  },
  "type": "module",
  "files": [
    "build/src",
    "build/index.d.ts",
    "build/index.js"
  ],
  "exports": {
    ".": "./build/index.js",
    "./types": "./build/src/types.js"
  },
  "homepage": "https://github.com/XScale-Agency/Package-Kit#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/XScale-Agency/Package-Kit.git"
  },
  "bugs": {
    "url": "https://github.com/XScale-Agency/Package-Kit/issues"
  },
  "scripts": {
    "clean": "del-cli build",
    "typecheck": "tsc --noEmit",
    "lint": "eslint .",
    "format": "prettier --write .",
    "quick:test": "node --import=./tsnode.esm.js --enable-source-maps bin/test.ts",
    "pretest": "yarn check",
    "test": "c8 yarn quick:test",
    "prebuild": "yarn check && yarn clean",
    "build": "tsc",
    "release": "yarn check && changeset version && changeset publish",
    "version": "yarn build",
    "prepublishOnly": "yarn build",
    "check": "yarn lint && yarn typecheck && yarn test",
    "postinstall": "husky",
    "prepack": "pinst --disable",
    "postpack": "pinst --enable",
    "playground": "node --import=./tsnode.esm.js --enable-source-maps bin/playground.ts"
  },
  "devDependencies": {
    "@adonisjs/assembler": "^7.7.0",
    "@adonisjs/core": "^6.12.0",
    "@adonisjs/eslint-config": "^2.0.0-beta.7",
    "@adonisjs/prettier-config": "^1.3.0",
    "@adonisjs/tsconfig": "^1.3.0",
    "@changesets/cli": "^2.27.9",
    "@japa/assert": "^3.0.0",
    "@japa/runner": "^3.1.4",
    "@swc/core": "^1.6.3",
    "@types/node": "^20.14.5",
    "c8": "^10.1.2",
    "copyfiles": "^2.4.1",
    "del-cli": "^5.1.0",
    "eslint": "^9",
    "husky": "^9.1.6",
    "pinst": "^3.0.0",
    "prettier": "^3.3.2",
    "ts-node-maintained": "^10.9.4",
    "typescript": "^5.4.5"
  },
  "packageManager": "yarn@4.5.0+sha512.837566d24eec14ec0f5f1411adb544e892b3454255e61fdef8fd05f3429480102806bac7446bc9daff3896b01ae4b62d00096c7e989f1596f2af10b927532f39",
  "author": "mohitxskull, xscale-agency",
  "license": "MIT",
  "keywords": [],
  "prettier": "@adonisjs/prettier-config",
  "publishConfig": {
    "access": "public",
    "tag": "latest"
  },
  "c8": {
    "exclude": [
      "tests/**"
    ]
  }
}
